{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Nuxt 3 (Dev)",
      "program": "${workspaceFolder}/node_modules/.bin/nuxi",
      "args": ["dev"],
      "autoAttachChildProcesses": true,
      "cwd": "${workspaceFolder}",
      "skipFiles": ["<node_internals>/**"],
      "smartStep": true,
      "sourceMaps": true,
      "env": {
        "NODE_ENV": "development"
      }
    }
  ]
}
